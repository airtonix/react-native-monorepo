{
  "name": "@airtonix-react-native/monorepo-template",
  "version": "1.0.0",
  "main": "index.js",
  "private": true,
  "license": "MIT",
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*",
      "tools/*",
      "generators/**/*",
      "!generators/**/template"
    ]
  },
  "scripts": {
    "clean": "git clean -xdf",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "types": "run-s -n -l 'types:*'",
    "types:root": "tsc",
    "types:apps": "run-p 'apps:* -- types'",
    "test": "run-p test:*",
    "test:apps": "run-p 'apps:* -- test'",
    "setup": "run-s setup:*",
    "setup:gems": "bundle install",
    "setup:apps": "yarn workspaces foreach --include 'app-*' run setup",
    "setup:husky": "husky install",
    "generate": "plop",
    "tool:provision": "yarn workspace @airtonix-react-native/provision do",
    "postinstall": "ultra setup",
    "apps:something": "yarn workspace @airtonix-react-native/something"
  },
  "dependencies": {
    "@babel/core": "^7.13.14",
    "@manypkg/cli": "^0.16.1",
    "@manypkg/get-packages": "^1.1.1",
    "@react-native-community/cli": "^5.0.1-alpha.2",
    "@types/jest": "^26.0.20",
    "@types/react": "^16.9.56",
    "@types/react-native": "^0.64.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "axios": "^0.21.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "dot-json": "^1.2.2",
    "dpx": "^3.1.1",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.1.2",
    "glob": "^7.1.6",
    "husky": "^6.0.0",
    "hygen": "^6.0.4",
    "jetifier": "^1.6.6",
    "jetify": "^1.0.2",
    "lint-staged": "^10.5.3",
    "lodash": "^4.17.21",
    "npm-run-all": "4.1.5",
    "onchange": "7.1.0",
    "plop": "^2.7.4",
    "prettier": "^2.2.1",
    "react": "^17.0.2",
    "react-native": "^0.64.0",
    "replace-in-files-cli": "^1.0.0",
    "run-if-diff": "^3.0.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.2",
    "ultra-runner": "^3.10.5",
    "version-changed": "^2.0.0"
  },
  "resolutions": {
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0"
  }
}
