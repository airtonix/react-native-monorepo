{
  "name": "@airtonix-react-native/app-{{ kebabCase title }}",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "run-p metro native:android",
    "ios": "run-p metro native:ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "native:android": "react-native run-android --no-packager",
    "native:ios": "react-native run-ios --no-packager",
    "metro": "react-native start --port 8081 --verbose",
    "gradle": "gradle --project-dir=android",
    "pod": "bundle exec pod --project-directory=ios",
    "setup": "run-s setup:*",
    "setup:gradle": "yarn gradle wrapper",
    "setup:pods": "yarn pod install",
    "types": "tsc"
  },
  "dependencies": {
    "react": "17.0.1",
    "react-native": "0.64.0"
  },
  "devDependencies": {
    "@airtonix-react-native/metro-config": "0.0.1",
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.20",
    "@types/react-native": "^0.64.0",
    "@types/react-test-renderer": "^16.9.2",
    "babel-jest": "^26.6.3",
    "eslint": "^7.14.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.64.0",
    "npm-run-all": "4.1.5",
    "react-test-renderer": "17.0.1",
    "typescript": "^3.8.3",
    "ultra-runner": "3.10.5"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
