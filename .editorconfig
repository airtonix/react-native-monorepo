# editorconfig.org
root = true

[*]
indent_style = space
indent_size = 2
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true
# See http://stackoverflow.com/a/729795

[*.md]
trim_trailing_whitespace = false

[apps/*/android/**/*]
indent_style = space
indent_size = 4
